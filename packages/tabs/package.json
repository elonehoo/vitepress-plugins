{
  "name": "@elonehoo/vitepress-plugin-tabs",
  "type": "module",
  "version": "0.0.2",
  "description": "A plugin that adds syntax for showing content in tabs.",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "keywords": [
    "vitepress",
    "plugin",
    "tabs"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    },
    "./client": "./src/client/index.ts"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "client.ts",
    "src/client"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "vitepress": "1.0.0-alpha.43",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "markdown-it": "^13.0.1"
  }
