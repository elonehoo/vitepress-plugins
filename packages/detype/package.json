{
  "name": "@elonehoo/vitepress-plugin-detype",
  "type": "module",
  "version": "0.0.2",
  "description": "A plugin that adds syntax for generating js from ts code snippet.",
  "author": "Elone Hoo <hi@elonehoo.me>",
  "license": "MIT",
  "keywords": [
    "vitepress",
    "plugin",
    "detype"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "@elonehoo/vitepress-plugin-tabs": "workspace:*",
    "detype": "^0.6.2",
    "vitepress": "1.0.0-alpha.43",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@types/markdown-it": "^12.2.3",
    "markdown-it": "^13.0.1",
    "vite": "^4.0.4"
  }
}
